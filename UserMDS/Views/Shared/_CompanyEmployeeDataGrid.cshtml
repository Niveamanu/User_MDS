@using UserMDS.Models
@model IEnumerable<CompanyEmployeeBenefitsModel>
@{
    Layout = null;
}
<div id="menu">
    <ul class="k-menu k-header k-reset k-widget k-menu-horizontal" id="Menu" data-role="menu"
        aria-orientation="horizontal" tabindex="0" role="menubar" aria-activedescendant="Menu_mn_active">
        <li class="k-item k-menu-item k-first" aria-haspopup="true" aria-expanded="false" role="menuitem"
            id="Menu_mn_active">
            <span class="k-link k-menu-link">
                <a href="/">

                    <span class="k-menu-link-text">Back To Dashboard</span>
                </a>
            </span>
        </li>

    </ul>

</div>
@if (Model.Count() > 0)
{

    <div class="tab-content" id="nav-tabContent">
        <div class="tab-pane fade  overflow-auto show active" id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">
            <table class="CompanyEmployeeBenefits table table-hover">
                <thead class="thead-light">
                    <tr>
                        <th>S.No</th>
                        <th>Plant Unique</th>
                        <th>Work Home Perc</th>
                        <th>Insurance Perc</th>
                        <th>Pr Tax perc</th>
                        <th>pto perc</th>
                        <th>adj factor perc</th>
                        <th>tot benefits perc</th>
                       


                    </tr>
                </thead>
                @{

                    int counter = 1;
                @foreach (CompanyEmployeeBenefitsModel result in Model)
                {
                    <tr>
                    <td>@counter</td>
                        <td>@result.plant_unique</td>
                        <td>@result.work_home_perc</td>
                        <td>@result.insurance_perc</td>
                        <td>@result.pr_tax_perc</td>
                        <td>@result.pto_perc</td>
                        <td>@result.adj_factor_perc</td>
                        <td>@result.tot_benefits_perc</td>


                    </tr>
                        counter++;
                    }
                }
            </table>
        </div>

    </div>

}
<script type="text/javascript">
    $(document).ready(function () {

        let table = new DataTable('.CompanyEmployeeBenefits', {
            dom: 'Bfrtip',
            buttons: [

                'excelHtml5'


            ]
        });


    });

</script>
